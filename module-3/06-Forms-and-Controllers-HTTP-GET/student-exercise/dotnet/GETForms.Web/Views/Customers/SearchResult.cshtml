@model IList<Customer>
@{
    ViewData["Title"] = "SearchResult";
}

<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Active</th>

        </tr>
    </thead>
    <tbody>
        <tr>
            @foreach (Customer customer in Model)
            {
                <td>@customer.FirstName @customer.LastName</td>
                <td>@customer.Email</td>
                <td>@customer.IsActive</td>
            }   

        </tr>
    </tbody>
</table>

